														</xs:complexType>
													</xs:element>
													<xs:element name="FUNCTION" maxOccurs="unbounded">
														<xs:complexType>
															<xs:sequence>
																<xs:element name="TASKID" type="xs:string" />
																<xs:element name="TASKNAME" type="xs:string" />
															</xs:sequence>
														</xs:complexType>
													</xs:element>
													<xs:element name="ACCOUNT" maxOccurs="unbounded">
														<xs:complexType>
															<xs:sequence>
																<xs:element name="ACCOUNTNO" type="xs:string" />
																<xs:element name="CURCD" type="xs:string" />
																<xs:element name="ACCOUNTTYPE" type="xs:string" />
																<xs:element name="TRANSTYPE" type="xs:string" />
															</xs:sequence>
														</xs:complexType>
													</xs:element>
													<xs:element name="TODOLIST" maxOccurs="unbounded">
