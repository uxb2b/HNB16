                                                </xs:sequence>
                                            </xs:complexType>
                                        </xs:element>
                                        <xs:element name="SvcRs">
                                            <xs:complexType>
                                                <xs:sequence>
                                                    <xs:element name="ACTNO" type="xs:string" />
                                                    <xs:element name="NAME" type="xs:string" />
                                                    <xs:element name="COMADR" type="xs:string" />
                                                    <xs:element name="FIDATE" type="xs:string" />
                                                    <xs:element name="ISTPRT" type="xs:string" />
                                                    <xs:element name="PUNINO" type="xs:string" />
                                                    <xs:element name="PENAME" type="xs:string" />
                                                </xs:sequence>
                                            </xs:complexType>
                                        </xs:element>
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>