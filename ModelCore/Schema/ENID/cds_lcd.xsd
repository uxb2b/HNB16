					</xs:annotation>
				</xs:element>
				<xs:element name="cpDoc" type="cds:CPType" minOccurs="0">
					<xs:annotation>
						<xs:documentation>本票</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="lcData" type="cds:LcdDataType" minOccurs="0">
					<xs:annotation>
						<xs:documentation>信用狀控制資料</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="version" type="xs:string" use="required"/>
