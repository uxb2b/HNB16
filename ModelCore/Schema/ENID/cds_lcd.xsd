<xs:schema targetNamespace="http://www.uxcds.com/xmlschema/cds" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:cds="http://www.uxcds.com/xmlschema/cds" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:include schemaLocation="cds_lcd_type.xsd"/>
	<xs:element name="CDSLcd">
		<xs:annotation>
			<xs:documentation>CDS國內信用狀</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="lcInfo" type="cds:LcdInfoType">
					<xs:annotation>
						<xs:documentation>信用狀資料</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="lcDoc" type="cds:LcdType">
					<xs:annotation>
