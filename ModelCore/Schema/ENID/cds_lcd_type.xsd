      	</xs:annotation>
      </xs:element>
      <xs:element name="lcaTail" type="xs:string" minOccurs="0" >
      	<xs:annotation>
      		<xs:documentation>開狀申請書結尾說明</xs:documentation>
      	</xs:annotation>
      </xs:element>
    </xs:sequence>
		<xs:attribute name="appNo" type="xs:string" use="required"/>
		<xs:attribute name="appDate" type="xs:string" use="required"/>
	</xs:complexType>
