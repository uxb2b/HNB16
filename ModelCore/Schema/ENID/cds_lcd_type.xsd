					<xs:documentation>開票銀行</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="cpAccount" type="xs:string">
				<xs:annotation>
					<xs:documentation>開票帳戶</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="payingBank" type="cds:BankType">
				<xs:annotation>
					<xs:documentation>付款銀行</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="payer" type="cds:CompanyType">
				<xs:annotation>
