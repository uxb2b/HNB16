	<xs:complexType name="LcdType">
		<xs:annotation>
			<xs:documentation>CDS國內信用狀本文資料類型</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="lcTitle" type="xs:string">
				<xs:annotation>
					<xs:documentation>信用狀名稱</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="lcType">
				<xs:annotation>
					<xs:documentation>信用狀類型</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="sight"/>
						<xs:enumeration value="usance"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="lcHeader" type="xs:string">
				<xs:annotation>
					<xs:documentation>信用狀起始說明</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="lcNo" type="xs:string">
				<xs:annotation>
					<xs:documentation>信用狀號碼</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="issuingDate" type="xs:string">
				<xs:annotation>
					<xs:documentation>開狀日期</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="issuingBank" type="cds:BankType">
				<xs:annotation>
					<xs:documentation>開狀銀行</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="advisingBank" type="cds:BankType">
				<xs:annotation>
					<xs:documentation>通知銀行</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="advisingDate" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>通知日期</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="advisingBankNo" type="xs:string">
				<xs:annotation>
					<xs:documentation>通知銀行編號</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="payingBank" type="cds:BankType">
				<xs:annotation>
					<xs:documentation>付款銀行</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="applicant" type="cds:CompanyType">
				<xs:annotation>
					<xs:documentation>申請人</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="beneficiary" type="cds:CompanyType">
				<xs:annotation>
					<xs:documentation>受益人</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="amount" type="cds:AmountType">
				<xs:annotation>
					<xs:documentation>金額</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="currency" type="cds:CurrencyType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>幣別</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="expiryDate">
				<xs:complexType>
					<xs:annotation>
						<xs:documentation>有效期限</xs:documentation>
					</xs:annotation>
					<xs:simpleContent>
						<xs:extension base="xs:string">
							<xs:attribute name="changed" use="optional">
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:enumeration value="true"/>
										<xs:enumeration value="false"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:attribute>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="lcConditions" minOccurs="0">
				<xs:annotation>
					<xs:documentation>信用狀條款</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="lcCond" type="xs:string" minOccurs="0" maxOccurs="100"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="lcAdditionalConditions" minOccurs="0">
				<xs:annotation>
					<xs:documentation>特別指示條款</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="lcAddCond" type="xs:string" minOccurs="0" maxOccurs="100"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element ref="cds:conditions" minOccurs="0">
				<xs:annotation>
					<xs:documentation>信用狀條款及特別指示條款</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="lcTail" type="xs:string">
				<xs:annotation>
					<xs:documentation>信用狀結尾說明</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="advisingBankNotes" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>通知銀行記載</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="LcdDataType">
		<xs:annotation>
			<xs:documentation>CDS國內信用狀控制資料類型</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="payment">
				<xs:annotation>
					<xs:documentation>付款期限</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="term">
							<xs:annotation>
								<xs:documentation>
atSight:見票即付
atDaysAfterSight:見票後定期付款
atDaysAfterDate:發票日後定期付款
onDate:定期付款
atDaysAfterBLDate:提單日期後定期付款
</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="atSight"/>
									<xs:enumeration value="atDaysAfterSight"/>
									<xs:enumeration value="atDaysAfterDate"/>
									<xs:enumeration value="onDate"/>
									<xs:enumeration value="atDaysAfterBLDate"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="days" type="xs:string" minOccurs="0"/>
						<xs:element name="date" type="xs:string" minOccurs="0"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="isPartialShipment">
				<xs:annotation>
					<xs:documentation>是否分批交貨</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="true"/>
						<xs:enumeration value="false"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="isCSCConditions">
				<xs:annotation>
					<xs:documentation>是否為中鋼之電子押匯特別指示條款</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="true"/>
						<xs:enumeration value="false"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="CSCSalesDept" type="xs:string">
				<xs:annotation>
					<xs:documentation>中鋼業務單位</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="LcdInfoType">
		<xs:annotation>
			<xs:documentation>
CDS國內信用狀資料類型
appNo:國內信用狀申請書編號
</xs:documentation>
		</xs:annotation>
