<xs:schema targetNamespace="http://www.uxcds.com/xmlschema/cds" xmlns:cds="http://www.uxcds.com/xmlschema/cds" xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:include schemaLocation="cds_data_type.xsd"/>
	<xs:element name="CDSDataBankCustomer">
		<xs:annotation>
			<xs:documentation>
CDS銀行客戶帳戶資料
version:版本號碼</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="bankCustomerInfo" type="cds:BankCustomerInfoType">
					<xs:annotation>
						<xs:documentation>銀行資訊</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="companyAccountItems" type="cds:CompanyAccountType">
					<xs:annotation>
						<xs:documentation>公司銀行帳戶資料</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="version" type="xs:string" use="required" fixed="1.0"/>
		</xs:complexType>
	</xs:element>
</xs:schema>
