{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Debug Step by Step",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/TestConsole/bin/Debug/net9.0/TestConsole.dll",
            "args": [
                "F:\\Project\\AppDev\\HNB\\Reference\\hncb_enid_data_csv",
                "(null)",
                "NULL",
                ".csv"
            ],
            "cwd": "${workspaceFolder}/TestConsole",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            // 使用 IntelliSense 找出 C# 偵錯具有哪些屬性
            // 針對現有屬性的描述使用暫留
            // 如需詳細資訊，請造訪 https://github.com/dotnet/vscode-csharp/blob/main/debugger-launchjson.md。
            "name": ".NET Core Launch (web)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build-webhome",
            // 如果您已變更目標 Framework，請務必更新程式路徑。
            "program": "${workspaceFolder}/WebHome/bin/Debug/net9.0/WebHome.dll",
            "args": [],
            "cwd": "${workspaceFolder}/WebHome",
            "stopAtEntry": false,
            // 啟用在 ASP.NET Core 開始時啟動網頁瀏覽器。如需詳細資訊: https://aka.ms/VSCode-CS-LaunchJson-WebBrowser
            "serverReadyAction": {
                "action": "openExternally",
                "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development",
                "ASPNETCORE_HOSTINGSTARTUPASSEMBLIES": "Microsoft.AspNetCore.Mvc.Razor.RuntimeCompilation",
                "ASPNETCORE_DETAILEDERRORS": "true"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/WebHome/Views"
            }
        },
    ]
}